  <script type="text/javascript">
    var pageMetricsUrl = 'https://xp.apple.com/report/2/xp_its_main?responseType=image&eventType=page&app=iTunes&pageId=DIHPodcastPage&pageType=finance&eventSource=server&page=finance_DIHPodcastPage';
    if (pageMetricsUrl) {
        pageMetricsUrl += "&eventTime=" + encodeURIComponent((new Date()).getTime());
    }
    var billingSection = document.querySelector(".edit-billing, .creditcard");
    if (billingSection !== null) {
        var selectedPaymentType = document.querySelector(".credit-card-picker input.selected, #paymentTypeField");
        if (selectedPaymentType !== null) {
            pageMetricsUrl += "&pageDetails=pmtTyp%3D" + selectedPaymentType.value;
        } 
    }
    if (its && its.cookies) {
        var itcCt = its.cookies.getUnescaped("itcCt");
        var itscc = its.cookies.getUnescaped("itscc");
        var hasitcCt = false;
        if (itcCt && itcCt !== "") {
            pageMetricsUrl += "&cookie=itcCt%3D" + itcCt;
            hasitcCt = true;
        }
        if (itscc && itscc != "") {
            pageMetricsUrl += (hasitcCt) ? "%26" : "&cookie=";
            pageMetricsUrl += "itscc%3D" + itscc;
        }
    }
    var containingDiv = document.createElement('div');
    containingDiv.innerHTML = "<img width='0' height='0' border='0' alt='' aria-hidden=true src='" + pageMetricsUrl + "'/>";
    document.body.appendChild(containingDiv);

    // NOTE: This should be considered temporary and replaced when a better solution is available.
    //      right now, SFMetrics should be referenced for all of these, but MZComponents can't access anything
    //      in MZStoreComponents so we're going to have to go with this workaround.
    var metricsConfig = {
        'metricsUrl' : 'https://xp.apple.com/report/2/',
        'compoundSeparator' : '_',
        'tokenSeparator' : '|',
        'postFrequency' : 60000,
        'metricsBase' : {}
    };
</script>

